<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<!-- ConfigWidget.ui.xml -->

<ui:UiBinder
  xmlns:b="urn:import:org.gwtbootstrap3.client.ui"
  xmlns:ui='urn:ui:com.google.gwt.uibinder'
  xmlns:g='urn:import:com.google.gwt.user.client.ui'>
   <ui:style src="ConfigWidget.css" />
      <g:FlowPanel>
       <b:Heading size="H1" text="Configuration club" styleName="{style.heading}"/>
       <b:ButtonGroup  ui:field='dropDownUserClubsButtonGroup' styleName='{style.clubBox}'>
        <b:Button dataToggle='DROPDOWN' text='Club' toggleCaret='true' ui:field='dropDownUserClubsButton'/>
        <b:DropDownMenu ui:field='dropDownUserClubs' />
       </b:ButtonGroup>
       <g:FormPanel ui:field="configEditForm" method="post" visible="false">
        <g:FlowPanel>
          <g:Hidden ui:field="current_session" name="current_session" />
          <g:Hidden ui:field="guid_on_form" name="guid" />
          <g:Hidden ui:field="dataToSave" name="data_to_save" />
        </g:FlowPanel>
       </g:FormPanel>
       <b:Container>
        <b:NavTabs>
         <b:TabListItem dataTarget="#club" active="true">Club</b:TabListItem>
         <b:TabListItem dataTarget="#session">Session</b:TabListItem>
         <b:TabListItem dataTarget="#cours">Cours</b:TabListItem>
         <b:TabListItem dataTarget="#prix">Prix</b:TabListItem>
         <b:TabListItem dataTarget="#escompte">Escompte</b:TabListItem>
         <b:TabListItem dataTarget="#produit">Ventes d&eacute;tail</b:TabListItem>
         <b:NavbarForm pull="RIGHT">
          <b:Button ui:field="retour" text="Retour page principale" />
         </b:NavbarForm>
        </b:NavTabs>
         <b:TabContent>
          <b:TabPane active="true" b:id="club">
             <b:Form type="HORIZONTAL">
              <b:FormGroup><b:FormLabel for="nom_club_tb" addStyleNames="col-md-2">Nom du club:</b:FormLabel><g:FlowPanel addStyleNames="col-md-4"><b:TextBox ui:field="nom_club" name="nom_club_tb"/></g:FlowPanel></b:FormGroup>
              <b:FormGroup><b:FormLabel for="numero_club_tb" addStyleNames="col-md-2">Numero club JQ:</b:FormLabel><g:FlowPanel addStyleNames="col-md-2"><b:TextBox ui:field="numero_club" name="numero_club_tb"/></g:FlowPanel></b:FormGroup>
              <b:FormGroup><b:FormLabel for="nom_short_tb" addStyleNames="col-md-2">Nom (abbrevi&eacute;):</b:FormLabel><g:FlowPanel addStyleNames="col-md-2"><b:TextBox ui:field="nom_short" name="nom_short_tb"/></g:FlowPanel></b:FormGroup>
              <b:FormGroup><b:FormLabel for="ville_tb" addStyleNames="col-md-2">Ville:</b:FormLabel><g:FlowPanel addStyleNames="col-md-4"><b:TextBox ui:field="ville" name="ville_tb"/></g:FlowPanel></b:FormGroup>
              <b:FormGroup><b:FormLabel for="prefix_codepostale_tb" addStyleNames="col-md-2">Prefixe code postale:</b:FormLabel><g:FlowPanel addStyleNames="col-md-2"><b:TextBox ui:field="prefix_codepostale" name="prefix_codepostale_tb"/></g:FlowPanel></b:FormGroup>
              <b:FormGroup><b:FormLabel for="indicatif_regional_tb" addStyleNames="col-md-2">Indicatif r&eacute;gional:</b:FormLabel><g:FlowPanel addStyleNames="col-md-2"><b:TextBox ui:field="indicatif_regional" name="indicatif_regional_tb"/></g:FlowPanel></b:FormGroup>
              <b:FormGroup><b:FormLabel for="escompte_resident_tb" addStyleNames="col-md-2">Escompte r&eacute;sident:</b:FormLabel><g:FlowPanel addStyleNames="col-md-2"><b:TextBox ui:field="escompte_resident" name="escompte_resident_tb"/></g:FlowPanel></b:FormGroup>
              <b:FormGroup><b:FormLabel for="supplement_prorata_tb" addStyleNames="col-md-2">Supplement prorata:</b:FormLabel><g:FlowPanel addStyleNames="col-md-2"><b:TextBox ui:field="supplement_prorata" name="supplement_prorata_tb"/></g:FlowPanel></b:FormGroup>
              <b:FormGroup><b:FormLabel for="default_prorata_tb" addStyleNames="col-md-2">Prorata par defaut:</b:FormLabel><g:FlowPanel addStyleNames="col-md-1"><b:CheckBox ui:field="default_prorata" name="default_prorata_tb"/></g:FlowPanel></b:FormGroup>
              <b:FormGroup><b:FormLabel for="afficher_paypal_tb" addStyleNames="col-md-2">Afficher supplement PayPal:</b:FormLabel><g:FlowPanel addStyleNames="col-md-1"><b:CheckBox ui:field="afficher_paypal" name="afficher_paypal_tb"/></g:FlowPanel></b:FormGroup>
             </b:Form>
          </b:TabPane>
          <b:TabPane b:id="session"><g:FlowPanel ui:field="sessionTab"/></b:TabPane>
          <b:TabPane b:id="cours"><g:FlowPanel ui:field="coursTab"/></b:TabPane>
          <b:TabPane b:id="prix"><g:FlowPanel ui:field="prixTab"/></b:TabPane>
          <b:TabPane b:id="escompte"><g:FlowPanel ui:field="escompteTab"/></b:TabPane>
          <b:TabPane b:id="produit"><g:FlowPanel ui:field="produitTab"/></b:TabPane>
         </b:TabContent>
       </b:Container>
      </g:FlowPanel>
</ui:UiBinder>

