<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<!-- ConfigWidget.ui.xml -->

<ui:UiBinder xmlns:ui='urn:ui:com.google.gwt.uibinder' xmlns:g='urn:import:com.google.gwt.user.client.ui'>
	<ui:style src="ConfigWidget.css" />
    <g:HTMLPanel ui:field="configMain">
 		<form id="configform" class="{style.compactform}" method="POST" target="eatFormSubmitResults">
    	</form>
    	<g:TabLayoutPanel ui:field="configTabPanel" barHeight="2" barUnit="EM">
    		<g:tab>
        		<g:header>Club</g:header>
        		<g:FlowPanel>
        			<g:Anchor ui:field='nom_club'>Nom du club:</g:Anchor>
        		</g:FlowPanel>
    		</g:tab>
    		<g:tab>
        		<g:header>Sessions</g:header>
        		<g:FlowPanel>
        			<g:Anchor ui:field='nom_session'>Nom du session:</g:Anchor> 
        	    </g:FlowPanel>                    
    		</g:tab>                        
		</g:TabLayoutPanel> 
    </g:HTMLPanel>
 
</ui:UiBinder>
