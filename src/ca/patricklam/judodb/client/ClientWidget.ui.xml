<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<!-- ClientWidget.ui.xml -->

<ui:UiBinder
  xmlns:b="urn:import:org.gwtbootstrap3.client.ui"
  xmlns:ui='urn:ui:com.google.gwt.uibinder'
  xmlns:g='urn:import:com.google.gwt.user.client.ui'>
	<ui:style src="ClientWidget.css" />
	<g:HTMLPanel ui:field="clientMain">
	  <b:ListBox ui:field="dropDownUserClubs" visibleItemCount="1" styleName="{style.userclub}" />
 		<div ui:field="cid" class="cid" />
 		<form id="clientform" class="{style.compactform}" method="POST" target="eatFormSubmitResults">
 		<g:Hidden ui:field="guid_on_form" name="guid" />
 		<g:Hidden ui:field="sid" name="sid" />
 		<g:Hidden ui:field="deleted" name="deleted" />
 		<g:Hidden ui:field="ddn" name="ddn" />
		<g:Hidden ui:field="club_id_encoded" name="club_id_encoded" />
 				
		<fieldset> <legend>Identit&eacute;</legend>
 		 <div class="{style.fourper}" id="identite">
		  <label><span class="noprint">Nom<br /></span><b:TextBox ui:field="nom" name="nom"/></label>
		  <label><span class="noprint">Prénom<br /></span><b:TextBox ui:field="prenom" name="prenom"/></label>
		  <label><span class="noprint">Date de naissance (jj/mm/aaaa)<br /></span><b:TextBox ui:field="ddn_display" name="ddn_display"/></label>
	      <label><span class="noprint">Sexe<br /></span><b:TextBox ui:field="sexe" name="sexe" /></label>
		 </div>
        </fieldset>

		<fieldset> 
		 <legend>Adresse<span id="copySib" class="noprint"><g:Anchor ui:field="copysib">&nbsp;(copier)</g:Anchor></span></legend>
 		 <div class="{style.oneper}">
		  <label><span class="noprint">Adresse<br /></span><b:TextBox ui:field="adresse" name="adresse" /></label>
         </div>
		 <div class="casMedical" style="width: 20em">
			<p>L'état médical du judoka inscrit requiert de remplir une fiche médicale plus approfondie.</p>
			<p style="padding-left: 1em">
 			<span style="border:solid">&nbsp;&nbsp;&nbsp;&nbsp;</span> OUI&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;
	 		<span style="border:solid">&nbsp;&nbsp;&nbsp;&nbsp;</span> NON
			</p>
			<p>Initiales ______</p>
		 </div>
 		 <div class="{style.fourper}">
		  <label><span class="noprint">Ville<br /></span><b:TextBox ui:field="ville" name="ville"/></label>
		  <label><span class="noprint">Code postal<br /></span><b:TextBox ui:field="codePostal" name="code_postal"/></label>
		  <label><span class="noprint">T&eacute;l&eacute;phone(s)<br /></span><b:TextBox ui:field="tel" name="tel"/></label>
  		 </div>
 		 <div class="{style.threeper}">
		  <label>T&eacute;l&eacute;phone urgence<br /><b:TextBox ui:field="tel_contact_urgence" name="tel_contact_urgence"/></label>
		  <label>Courriel<br /><b:TextBox ui:field="courriel"  name="courriel"/></label>
 		 </div>
		</fieldset>

 		<fieldset> <legend>Autres renseignements</legend>
 		 <div class="{style.fourper}">
		  <label>Num&eacute;ro Judo Qu&eacute;bec<br /><b:TextBox ui:field="affiliation"  name="affiliation"/></label>
		  <label>Grade judo <span class="noprint">(<g:Anchor ui:field="showgrades">histoire</g:Anchor>)</span><br /><b:TextBox ui:field="grade"/></label>
	      <label>Date du grade<br /><b:TextBox ui:field="date_grade"/></label>
	      <g:Hidden ui:field="grades_encoded" name="grades_encoded" />
	      <g:Hidden ui:field="grade_dates_encoded" name="grade_dates_encoded" />
		  <label style="font-size:80%">N° carte municipale<br /><b:TextBox ui:field="carte_resident" name="carte_resident" /></label>
   	     </div>
 		 <div class="{style.oneper}">
		  <label>Nom re&ccedil;u imp&ocirc;t<br /><b:TextBox ui:field="nom_recu_impot" name="nom_recu_impot" /></label>
		 </div>
 		</fieldset>

 		<fieldset> <legend>Inscription</legend>
		 <div class="{style.fiveper}">
   		  <label>Date d'inscription
           <span class="noprint"><g:InlineLabel ui:field="semaines"></g:InlineLabel></span> <g:Anchor ui:field="inscrire"><span class="noprint">(inscrire)</span></g:Anchor> <g:Anchor ui:field="modifier"><span class="noprint">(modifier)</span></g:Anchor> <g:Anchor ui:field="desinscrire"><span id="desinscrire" class="noprint">(désinscrire)</span></g:Anchor><br />
           <b:ListBox ui:field="date_inscription" /> <g:Hidden ui:field="date_inscription_encoded" name="date_inscription_encoded" />
          </label>
		  <label>Cat&eacute;gorie d'&acirc;ge<br /><b:TextBox ui:field="categorie" /></label>
		  <label>Saisons<br /><b:TextBox ui:field="saisons"  /> <g:Hidden ui:field="saisons_encoded" name="saisons_encoded" /></label>
   		  <label style="font-size:80%"><span class="noprint">Affiliation Judo QC envoy&eacute;<br /><g:CheckBox ui:field="verification"/></span> <g:Hidden ui:field="verification_encoded" name="verification_encoded" />
 		  </label>
		  <label><span class="noprint">Prorata<br /><g:CheckBox ui:field="prorata"/></span> <g:Hidden ui:field="prorata_encoded" name="prorata_encoded" />
		  </label>
  		 </div>
  		
  		 <div class="{style.fourper}">
		  <label style="width:46%">Cours<br /><b:ListBox ui:field="cours"/><g:Hidden ui:field="cours_encoded" name="cours_encoded" /></label>
		  <label># sessions<br /><b:ListBox ui:field="no_sessions"/><g:Hidden ui:field="no_sessions_encoded" name="no_sessions_encoded" /></label>
		  <span class="{style.fraisOnRight}"><label>Frais cours<br /><b:TextBox ui:field="categorieFrais" /> 
   		    <g:Hidden ui:field="categorieFrais_encoded" name="categorie_frais_encoded"/></label></span>
  		 </div>
  		 <div class="{style.fourper}">
		  <label>Escompte<br /><b:ListBox ui:field="escompte"/><g:Hidden ui:field="escompte_encoded" name="escompte_encoded"/></label>
          <label>Raison cas sp&eacute;cial<br /><b:TextBox ui:field="cas_special_note"/><g:Hidden ui:field="cas_special_note_encoded" name="cas_special_note_encoded"/></label>
		  <label>% escompte<br /><b:TextBox ui:field="cas_special_pct"  /><g:Hidden ui:field="cas_special_pct_encoded" name="cas_special_pct_encoded"/></label>
          <span class="{style.fraisOnRight}"><label> - escompte<br /><b:TextBox ui:field="escompteFrais"  /><g:Hidden ui:field="escompteFrais_encoded" name="escompte_special_encoded"/></label></span>
         </div>
         <div class="{style.fourper}">
   		  <label>Déjà affili&eacute; Judo Qu&eacute;bec 2014-15<br /><g:CheckBox ui:field="sans_affiliation"/> <g:Hidden ui:field="sans_affiliation_encoded" name="sans_affiliation_encoded" /></label>
   		  <label>Affiliation &eacute;cole<br /><g:CheckBox ui:field="affiliation_ecole"/> <g:Hidden ui:field="affiliation_ecole_encoded" name="affiliation_ecole_encoded" /></label>
   		  <label>Affiliation initiation<br /><g:CheckBox ui:field="affiliation_initiation"/> <g:Hidden ui:field="affiliation_initiation_encoded" name="affiliation_initiation_encoded" /></label>
		  <span class="{style.fraisOnRight}"><label>Affiliation Judo Qu&eacute;bec<br /><b:TextBox ui:field="affiliationFrais"  /> <g:Hidden ui:field="affiliationFrais_encoded" name="affiliation_frais_encoded" /></label></span>
         </div>
         <div class="{style.fourper}" style="border-bottom: 1px grey;">
          <label>Co&ucirc;t judogi<br /><b:ListBox ui:field="produit"/> <g:Hidden ui:field="judogi_encoded" name="judogi_encoded" /></label>
          <label id="rabais_resident_label">Rabais r&eacute;sident<br /><g:CheckBox ui:field="resident"/><g:Hidden ui:field="resident_encoded" name="resident_encoded" /></label>
          <label id="frais_paypal_label">Frais PayPal<br /><g:CheckBox ui:field="paypal"/><g:Hidden ui:field="paypal_encoded" name="paypal_encoded" /></label>
          <span class="{style.fraisOnRight}"><label>Autres frais<br /><b:TextBox ui:field="suppFrais"  /><g:Hidden ui:field="suppFrais_encoded" name="supp_frais_encoded" /></label></span>
        </div>
        <div class="{style.fourper}">
          <label>Paiement complet&eacute;<br /><g:CheckBox ui:field="solde"/><g:Hidden ui:field="solde_encoded" name="solde_encoded" /></label>
          <span class="{style.fraisOnRight}"><label>Frais total<br /><b:TextBox ui:field="frais"  /><g:Hidden ui:field="frais_encoded" name="frais_encoded" /></label></span>
        </div>
       </fieldset>
      <b:Button ui:field='saveAndReturnClientButton'>Sauvegarder et retourner</b:Button>
      <b:Button ui:field='saveClientButton'>Sauvegarder</b:Button>
      <span style="position: absolute; right:40%"><b:Button ui:field='deleteClientButton'>Supprimer</b:Button></span>
      <b:Button ui:field='discardClientButton' addStyleNames='{style.floatRight}'>Annuler</b:Button>
      </form>

      <iframe style="display:none" name="eatFormSubmitResults" />

      <g:HTMLPanel ui:field="blurb" styleName="blurb" />

	  <g:HTMLPanel ui:field="gradeHistory" styleName="{style.gradeHistory}">
       <div class="noprint">
        <h4 id="gradeHeader">Histoire grades</h4>
        <g:Grid ui:field="gradeTable" />
        <div class="{style.actions}" style="float:left; text-align:left">
         <g:Anchor ui:field="saveGrades">Sauvegarder</g:Anchor>
        </div>
        <div class="{style.actions}" style="float:right; text-align:right">
         <g:Anchor ui:field="annulerGrades">Annuler</g:Anchor>
        </div>
       </div>
      </g:HTMLPanel>
    
    </g:HTMLPanel>
 
</ui:UiBinder>
