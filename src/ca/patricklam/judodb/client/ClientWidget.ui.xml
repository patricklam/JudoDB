<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<!-- ClientWidget.ui.xml -->

<ui:UiBinder
  xmlns:b="urn:import:org.gwtbootstrap3.client.ui"
  xmlns:b3="urn:import:org.gwtbootstrap3.extras.toggleswitch.client.ui"
  xmlns:bs="urn:import:org.gwtbootstrap3.extras.select.client.ui"
  xmlns:ui='urn:ui:com.google.gwt.uibinder'
  xmlns:g='urn:import:com.google.gwt.user.client.ui'>
	<ui:style src="ClientWidget.css" />
	<g:HTMLPanel ui:field="clientMain">
 		<div ui:field="cid" class="cid" />
		<b:Form ui:field="clientform" type="HORIZONTAL" method="POST"> <g:HTMLPanel>
 		<g:Hidden ui:field="guid_on_form" name="guid" />
 		<g:Hidden ui:field="sid" name="sid" />
 		<g:Hidden ui:field="deleted" name="deleted" />
 		<g:Hidden ui:field="ddn" name="ddn" />
		<g:Hidden ui:field="club_id_encoded" name="club_id_encoded" />

		<b:FieldSet b:id="identite"> <b:Legend>Identit&eacute;</b:Legend>
                  <b:FormGroup>
		   <b:Column size="XS_4"><b:FormLabel for="nom" addStyleNames="hidden-print">Nom</b:FormLabel><b:TextBox ui:field="nom" name="nom"/></b:Column>
		   <b:Column size="XS_4"><b:FormLabel for="prenom" addStyleNames="hidden-print">Prénom</b:FormLabel><b:TextBox ui:field="prenom" name="prenom" /></b:Column>
		   <b:Column size="XS_2"><b:FormLabel for="ddn_display" addStyleNames="hidden-print">Date de naissance</b:FormLabel><b:TextBox ui:field="ddn_display" name="ddn_display" placeholder="jj/mm/aa" /> </b:Column>
		   <b:Column size="XS_1"><b:FormLabel for="sexe" addStyleNames="hidden-print">Sexe</b:FormLabel><b:TextBox ui:field="sexe" name="sexe" /></b:Column>
		  </b:FormGroup>
                </b:FieldSet>

		<b:FieldSet><b:Legend>Adresse</b:Legend>
		<b:FormGroup>
		  <b:Column size="XS_10"><b:FormLabel for="adresse" addStyleNames="hidden-print">Adresse</b:FormLabel><b:TextBox ui:field="adresse" name="adresse" /></b:Column>
		  <b:Column size="XS_1"><b:FormLabel for="copysib" /><b:Button ui:field="copysib" addStyleNames="hidden-print offset-col-xs-1">copier</b:Button></b:Column>
		</b:FormGroup>
		<g:HTMLPanel>
		  <div class="{style.casMedical}" style="width: 20em">
		    <p>L'état médical du judoka inscrit requiert de remplir une fiche médicale plus approfondie.</p>
		    <p style="padding-left: 1em">
		      <span style="border:solid">&nbsp;&nbsp;&nbsp;&nbsp;</span> OUI&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;
		      <span style="border:solid">&nbsp;&nbsp;&nbsp;&nbsp;</span> NON
		    </p>
		    <p>Initiales ______</p>
		  </div>
		</g:HTMLPanel>
                 <b:FormGroup>
		   <b:Column size="XS_2"><b:FormLabel for="ville" addStyleNames="hidden-print">Ville</b:FormLabel><b:TextBox ui:field="ville" name="ville"/></b:Column>
		   <b:Column size="XS_2"><b:FormLabel for="code_postal" addStyleNames="hidden-print">Code postal</b:FormLabel><b:TextBox ui:field="codePostal" name="code_postal"/></b:Column>
		   <b:Column size="XS_3"><b:FormLabel for="tel" addStyleNames="hidden-print">T&eacute;l&eacute;phone(s)</b:FormLabel><b:TextBox ui:field="tel" name="tel"/></b:Column>
		 </b:FormGroup>
                 <b:FormGroup>
		   <b:Column size="XS_3"><b:FormLabel for="tel_contact_urgence">T&eacute;l&eacute;phone urgence</b:FormLabel><b:TextBox ui:field="tel_contact_urgence" name="tel_contact_urgence"/></b:Column>
		   <b:Column size="XS_4"><b:FormLabel for="courriel">Courriel</b:FormLabel><b:TextBox ui:field="courriel" name="courriel"/></b:Column>
		 </b:FormGroup>
		</b:FieldSet>

		<b:FieldSet> <b:Legend>Autres renseignements</b:Legend>
		 <b:FormGroup>
		  <b:Column size="XS_3"><b:FormLabel for="affiliation">Num&eacute;ro Judo Canada</b:FormLabel><b:TextBox ui:field="affiliation"  name="affiliation" addStyleNames="{style.six-em}"/></b:Column>
		  <b:Column size="XS_2"><g:HTMLPanel><label for="grade" class="control-label">Grade judo <span class="hidden-print">(<g:Anchor ui:field="showgrades">histoire</g:Anchor>)</span></label></g:HTMLPanel><b:ListBox ui:field="grade" addStyleNames="{style.nine-em}"/></b:Column>
	          <b:Column size="XS_3"><b:FormLabel for="date_grade">Date du grade</b:FormLabel><b:TextBox ui:field="date_grade" addStyleNames="{style.nine-em}" placeholder="jj/mm/aa"/></b:Column>
		  <g:Hidden ui:field="grades_encoded" name="grades_encoded" />
		  <g:Hidden ui:field="grade_dates_encoded" name="grade_dates_encoded" />
		  <b:Column size="XS_2"><b:FormLabel for="carte_resident">N° carte municipale</b:FormLabel><b:TextBox ui:field="carte_resident" name="carte_resident"/></b:Column>
		</b:FormGroup>
		 <b:FormGroup>
		   <b:Column size="XS_10"><b:FormLabel for="nom_recu_impot">Nom parent</b:FormLabel><b:TextBox ui:field="nom_recu_impot" name="nom_recu_impot" /></b:Column>
		 </b:FormGroup>
		 <b:FormGroup>
		   <b:Column size="XS_10"><b:FormLabel for="notes">Notes</b:FormLabel><b:TextBox ui:field="notes" name="notes" /></b:Column>
		 </b:FormGroup>
		</b:FieldSet>

		<b:FieldSet> <b:Legend>Inscription</b:Legend>
		<b:FormGroup>
		  <b:Column size="XS_3">
		    <g:HTMLPanel><label class="control-label" for="date_inscription">Date d'inscription <g:Anchor ui:field="inscrire" addStyleNames="hidden-print">(inscrire)</g:Anchor> <g:Anchor ui:field="modifier" addStyleNames="hidden-print">(modifier)</g:Anchor> <g:Anchor ui:field="desinscrire" addStyleNames="hidden-print">(désinscrire)</g:Anchor></label><g:HTMLPanel />
		    <b:ListBox ui:field="date_inscription" /> <g:Hidden ui:field="date_inscription_encoded" name="date_inscription_encoded" />
		    </g:HTMLPanel>
		  </b:Column>
		  <b:Column size="XS_1"><b:FormLabel for="sessions">Sessions</b:FormLabel><g:HTMLPanel /><b:ListBox ui:field="sessions" addStyleNames="{style.session-padding} tiny-print" /> <g:Hidden ui:field="saisons_encoded" name="saisons_encoded" /></b:Column>
		  <b:Column size="XS_1"><b:FormLabel for="categorie">Division</b:FormLabel><g:HTMLPanel /><b:TextBox ui:field="categorie" addStyleNames="{style.six-em}"/></b:Column>
		  <b:Column size="XS_3"><b:FormLabel for="cours">Cours</b:FormLabel><b:ListBox ui:field="cours"/><g:Hidden ui:field="cours_encoded" name="cours_encoded" /></b:Column>
		  <b:Column size="XS_2">
		    <g:HTMLPanel ui:field="prorata_group"><label class="control-label" for="prorata">Prorata <span class="hidden-print"><g:InlineLabel ui:field="semaines"/></span></label><g:HTMLPanel /><b3:ToggleSwitch ui:field="prorata" onIcon="CHECK" offIcon="TIMES"/> <g:Hidden ui:field="prorata_encoded" name="prorata_encoded" /></g:HTMLPanel>
		  </b:Column>
		  <b:Column size="XS_2" addStyleNames="pull-right"><b:FormLabel for="categorieFrais">Frais cours</b:FormLabel><b:TextBox ui:field="categorieFrais" /> <g:Hidden ui:field="categorieFrais_encoded" name="categorie_frais_encoded"/></b:Column>

		</b:FormGroup>
		<b:FormGroup ui:field="tarif_group">
		  <b:Column size="XS_4"><b:FormLabel for="tarif">Tarif</b:FormLabel><b:ListBox ui:field="tarif"/><g:Hidden ui:field="nom_tarif_id_encoded" name="nom_tarif_id_encoded"/></b:Column>
		</b:FormGroup>
		<b:FormGroup>
		  <b:Column size="XS_2"><b:FormLabel for="escompte">Escompte</b:FormLabel><b:ListBox ui:field="escompte"/><g:Hidden ui:field="escompte_encoded" name="escompte_encoded"/></b:Column>
		  <b:Column size="XS_3"><b:FormLabel for="cas_special_note">Raison cas sp&eacute;cial</b:FormLabel><b:TextBox ui:field="cas_special_note"/><g:Hidden ui:field="cas_special_note_encoded" name="cas_special_note_encoded"/></b:Column>
		  <b:Column size="XS_2"><b:FormLabel for="cas_special_pct">% escompte</b:FormLabel><b:TextBox ui:field="cas_special_pct" addStyleNames="{style.six-em}"  /><g:Hidden ui:field="cas_special_pct_encoded" name="cas_special_pct_encoded"/></b:Column>
		  <b:Column size="XS_2" addStyleNames="pull-right"><b:FormLabel for="escompteFrais"> - escompte</b:FormLabel><b:TextBox ui:field="escompteFrais"  /><g:Hidden ui:field="escompteFrais_encoded" name="escompte_special_encoded"/></b:Column>
		</b:FormGroup>

		<b:FormGroup>
		  <b:Column addStyleNames="hidden-print" size="XS_2"><b:FormLabel for="affiliation_envoye">Affiliation Judo QC/CA envoy&eacute;</b:FormLabel><g:HTMLPanel /><b3:ToggleSwitch ui:field="affiliation_envoye" onIcon="CHECK" offIcon="TIMES"/> <g:Hidden ui:field="affiliation_envoye_encoded" name="affiliation_envoye_encoded" /></b:Column>
		  <b:Column size="XS_2"><b:FormLabel for="date_affiliation_envoye">Date affiliation envoy&eacute;</b:FormLabel><g:HTMLPanel /><b:TextBox ui:field="date_affiliation_envoye" addStyleNames="{style.nine-em}" placeholder="jj/mm/aa"/><g:Hidden ui:field="date_affiliation_envoye_encoded" name="date_affiliation_envoye_encoded" /></b:Column>
		  <b:Column size="XS_2"><b:FormLabel for="carte_judoca_recu">Carte JudoCA re&ccedil;u</b:FormLabel><b3:ToggleSwitch ui:field="carte_judoca_recu" onIcon="CHECK" offIcon="TIMES"/> <g:Hidden ui:field="carte_judoca_recu_encoded" name="carte_judoca_recu_encoded" /></b:Column>
		  <b:Column size="XS_2"><b:FormLabel for="affiliation_speciale">Affiliation sp&eacute;ciale</b:FormLabel><g:HTMLPanel /><b:ListBox ui:field="affiliation_speciale"><g:item>---</g:item><g:item>Initiation</g:item><g:item>Scolaire</g:item><g:item>Parascolaire</g:item><g:item>D&eacute;j&agrave; affili&eacute;</g:item></b:ListBox> <g:Hidden ui:field="sans_affiliation_encoded" name="sans_affiliation_encoded" /><g:Hidden ui:field="affiliation_ecole_encoded" name="affiliation_ecole_encoded" /><g:Hidden ui:field="affiliation_parascolaire_encoded" name="affiliation_parascolaire_encoded" /><g:Hidden ui:field="affiliation_initiation_encoded" name="affiliation_initiation_encoded" /></b:Column>
		  <b:Column size="XS_2" addStyleNames="pull-right"><b:FormLabel for="affiliationFrais">Affiliation JQ+JC</b:FormLabel><b:TextBox ui:field="affiliationFrais"/> <g:Hidden ui:field="affiliationFrais_encoded" name="affiliation_frais_encoded" /></b:Column>
		</b:FormGroup>
		<b:FormGroup>
		  <b:Column size="XS_3"><b:FormLabel for="judogi">Passeports / Achats</b:FormLabel><bs:MultipleSelect noneSelectedText="Aucun" ui:field="produit"/> <g:Hidden ui:field="judogi_encoded" name="judogi_encoded" /></b:Column>
		  <b:Column size="XS_2" ui:field="rabais_resident_label"><b:FormLabel for="resident">Rabais r&eacute;sident</b:FormLabel><b3:ToggleSwitch ui:field="resident" onIcon="CHECK" offIcon="TIMES"/><g:Hidden ui:field="resident_encoded" name="resident_encoded" /></b:Column>
		  <b:Column size="XS_2" ui:field="frais_paypal_label"><b:FormLabel for="paypal">Frais suppl&eacute;ment</b:FormLabel><g:HTMLPanel /><b3:ToggleSwitch ui:field="paypal" onIcon="CHECK" offIcon="TIMES"/><g:Hidden ui:field="paypal_encoded" name="paypal_encoded" /></b:Column>
		  <b:Column size="XS_2" addStyleNames="pull-right"><b:FormLabel for="suppFrais">Autres frais</b:FormLabel><b:TextBox ui:field="suppFrais"  /><g:Hidden ui:field="suppFrais_encoded" name="supp_frais_encoded" /></b:Column>
		</b:FormGroup>
		<b:FormGroup>
		  <b:Column size="XS_2"><b:FormLabel for="solde">Paiement compl&eacute;t&eacute;</b:FormLabel><b3:ToggleSwitch ui:field="solde" onIcon="CHECK" offIcon="TIMES"/><g:Hidden ui:field="solde_encoded" name="solde_encoded" /></b:Column>
		  <b:Column size="XS_2"><b:FormLabel for="restant">Solde &agrave; payer</b:FormLabel><b:TextBox ui:field="restant" /></b:Column>
		  <b:Column size="XS_2" addStyleNames="pull-right"><b:FormLabel for="frais">Frais total</b:FormLabel><b:TextBox ui:field="frais"  /><g:Hidden ui:field="frais_encoded" name="frais_encoded" /></b:Column>
		</b:FormGroup>
       </b:FieldSet>
       <g:Hidden ui:field="paiements_encoded" name="paiements_encoded" />
       <b:FieldSet ui:field="paiements"> <b:Legend>Paiements</b:Legend>
       </b:FieldSet>
       <b:Row addStyleNames="hidden-print">
	 <b:Column size="XS_3"><b:Button ui:field='saveAndReturnClientButton'>Sauvegarder et retourner</b:Button></b:Column>
	 <b:Column size="XS_3"><b:Button ui:field='saveClientButton'>Sauvegarder</b:Button></b:Column>
	 <g:FlowPanel addStyleNames="col-xs-offset-5"><b:Column size="XS_2"><b:Button ui:field='deleteClientButton'>Supprimer</b:Button></b:Column></g:FlowPanel>
	 <b:Column size="XS_2" addStyleNames="pull-right"><b:Button ui:field='discardClientButton' addStyleNames='{style.floatRight}'>Annuler</b:Button></b:Column>
       </b:Row>
    </g:HTMLPanel>
    <g:HTMLPanel ui:field="blurb" styleName="blurb" />
    </b:Form>

      <g:HTMLPanel ui:field="gradeHistory" styleName="{style.gradeHistory}">
       <div class="hidden-print">
        <h4 id="gradeHeader">Histoire grades</h4>
        <g:Grid ui:field="gradeTable" />
        <div style="float:left; text-align:left">
         <g:Anchor ui:field="saveGrades">Sauvegarder</g:Anchor>
        </div>
        <div style="float:right; text-align:right">
         <g:Anchor ui:field="annulerGrades">Annuler</g:Anchor>
        </div>
       </div>
      </g:HTMLPanel>
      <b:ButtonGroup ui:field='dropDownUserClubsButtonGroup' addStyleNames="{style.clubBox} hidden-print">
	<b:Button dataToggle='DROPDOWN' text='Club' toggleCaret='true' ui:field='dropDownUserClubsButton'/>
	<b:DropDownMenu ui:field='dropDownUserClubs' />
      </b:ButtonGroup>
    
    </g:HTMLPanel>
 
</ui:UiBinder>
