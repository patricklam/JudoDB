<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<!-- LoginWidget.ui.xml -->

<ui:UiBinder xmlns:ui='urn:ui:com.google.gwt.uibinder' xmlns:g='urn:import:com.google.gwt.user.client.ui'>

 <g:HTMLPanel>
    <form class="compactform" action="#">
      Veuillez vous identifier: <br /><br />
      <div class="oneper"><label for="loginid">id</label><g:TextBox ui:field="loginid"/></div>
      <div class="oneper"><label for="password">mot de passe</label><g:PasswordTextBox ui:field="password"/></div>
      <div class="actions" id="loginActionsContainer">
        <g:Button ui:field="entrer">entrer</g:Button>
      </div>
    </form>
 </g:HTMLPanel>

</ui:UiBinder>
