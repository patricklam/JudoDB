       <g:HTMLPanel>
 		<form name="client" class="compactform">

		<fieldset> <legend>Identit&eacute;</legend>
 		 <div class="fourper" id="identite">
 		  <label for="{nom.getElement.getId}"><span class="noprint">Nom</span><g:TextBox ui:field="nom"/></label>
  		  <label for="{prenom.getElement.getId}"><span class="noprint">Prénom</span><g:TextBox ui:field="prenom"/></label>
  		  <label for="{ddn.getElement.getId}"><span class="noprint">Date de naissance (aaaa-mm-jj)</span><g:TextBox ui:field="ddn"/></label>
	      <label for="{sexe.getElement.getId}"><span class="noprint">Sexe</span><g:TextBox ui:field="sexe"/></label>
		 </div>
        </fieldset>

		<fieldset> 
		 <legend>Adresse<span id="copySib" class="noprint" style="display:none"> (<a href="#" onclick="copySibling(); return false">copier</a>)</span></legend>
 		 <div class="oneper">
  		  <label for="{adresse.getElement.getId}"><span class="noprint">Adresse</span><g:TextBox ui:field="adresse" visibleLength="80"/></label>
         </div>
 		 <div class="fourper">
  		  <label for="{ville.getElement.getId}"><span class="noprint">Ville</span><g:TextBox ui:field="ville"/></label>
  		  <label for="{codePostal.getElement.getId}"><span class="noprint">Code postal</span><g:TextBox ui:field="codePostal"/></label>
  		  <label for="{tel.getElement.getId}">T&eacute;l&eacute;phone(s)<g:TextBox ui:field="tel"/></label>
		  <label for="{courriel.getElement.getId}">Courriel<g:TextBox ui:field="courriel"/></label>
 		 </div>
		</fieldset>

 		<fieldset> <legend>Autres renseignments</legend>
 		 <div class="fourper">
   		  <label for="{affiliation.getElement.getId}">Num&eacute;ro Judo Qu&eacute;bec<g:TextBox ui:field="affiliation"/></label>
   		  <label for="{grade.getElement.getId}">Grade judo <span class="noprint">(<a href="#" onclick="showGradeHistory(); return false">histoire</a>)</span><g:TextBox ui:field="grade"/></label>
	      <label for="{date_grade.getElement.getId}">Date du grade<g:TextBox ui:field="date_grade"/></label>
   		  <label for="{carte_anjou.getElement.getId}">N° arrondissement Anjou<g:TextBox ui:field="carte_anjou"/></label>
   	     </div>
 		 <div class="twoper">
   		  <label for="{nom_recu_impot.getElement.getId}">Nom re&ccedil;u imp&ocirc;t<g:TextBox ui:field="nom_recu_impot" visibleLength="40"/>
 		  </label>
		 </div>
 		</fieldset>

 		<fieldset> <legend>Urgence</legend>
  		 <div class="twoper">
   		  <label for="{nom_contact_urgence.getElement.getId}">Contact en cas d'urgence<g:TextBox ui:field="nom_contact_urgence" visibleLength="40"/></label>
   		  <label for="{tel_contact_urgence.getElement.getId}">T&eacute;l&eacute;phone urgence<g:TextBox ui:field="tel_contact_urgence"/></label>
 		 </div>
 		</fieldset>

 		<fieldset> <legend>Inscription</legend>
  		 <div class="fourper">
   		  <label for="{date_inscription.getElement.getId}">Date d'inscription
           <g:Anchor ui:field="today"><span class="noprint">(aujourd'hui)</span></g:Anchor> <span id="desinscrire" class="noprint">(désinscrire)</span>
           <g:TextBox ui:field="date_inscription"/>
          </label>
   		  <label for="{categorie.getElement.getId}">Cat&eacute;gorie d'&acirc;ge<g:TextBox ui:field="categorie"/></label>
   		  <label for="{saisons.getElement.getId}">Saisons<g:TextBox ui:field="saisons"/>
   		  </label>
   		  <label for="{verification.getElement.getId}">Affiliation Judo QC verifi&eacute;<g:CheckBox ui:field="verification"/>
 		  </label>
  		 </div>
  		
  		 <div class="fourper">
   		  <label for="{cours.getElement.getId}" style="width:46%">Cours<g:ListBox ui:field="cours"/></label>
   		  <label for="{sessions.getElement.getId}">Sessions<g:ListBox ui:field="sessions"/></label>
   		  <label for="{categorieFrais.getElement.getId}" style="float:right">Frais cours<g:TextBox ui:field="categorieFrais"/></label>
  		 </div>
  		 <div class="fourper">
   		  <label for="{escompte.getElement.getId}">Escompte<g:ListBox ui:field="escompte"/></label>
          <label for="{cas_special_note.getElement.getId}">Raison reduction cas sp&eacute;cial<g:TextBox ui:field="cas_special_note"/></label>
   		  <label for="{cas_special_pct.getElement.getId}">Pourcentage escompte cas sp&eacute;cial<g:TextBox ui:field="cas_special_pct"/></label>
          <label for="{escompteFrais.getElement.getId}" style="float:right"> - escompte<g:TextBox ui:field="escompteFrais"/></label>
         </div>
         <div class="fourper">
   		  <label for="{sans_affiliation.getElement.getId}">Déjà affili&eacute; Judo Qu&eacute;bec 2011-12<g:CheckBox ui:field="sans_affiliation"/></label>
          <label for="{affiliationFrais.getElement.getId}" style="float:right">Affiliation Judo Qu&eacute;bec<g:TextBox ui:field="affiliationFrais"/></label>
         </div>
         <div class="fourper">
          <label for="{judogi.getElement.getId}">Co&ucirc;t judogi<g:TextBox ui:field="judogi"/></label>
          <label for="{passeport.getElement.getId}">Passeport Judo Qu&eacute;bec (5 $)<g:CheckBox ui:field="passeport"/></label>
          <label for="{non_anjou.getElement.getId}">Pas de carte Anjou (5 $)<g:CheckBox ui:field="non_anjou"/></label>
          <label for="{suppFrais.getElement.getId}" style="float:right">Autres frais<g:TextBox ui:field="suppFrais"/></label>
        </div>
        <div class="fourper">
          <label for="{frais.getElement.getId}" style="float:right">Frais total<g:TextBox ui:field="frais"/></label>
        </div>
       </fieldset>
      </form>
      <g:Button ui:field='saveClientButton'>Sauvegarder</g:Button>
      <g:Button ui:field='discardClientButton'>Annuler</g:Button>

      </g:HTMLPanel>

