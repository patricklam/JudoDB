       <g:HTMLPanel>
 		<div ui:field="cid" class="{style.cid}" />

 		<form class="{style.compactform}">
 		
		<fieldset> <legend>Identit&eacute;</legend>
 		 <div class="{style.fourper}" id="identite">
 		  <label><span class="noprint">Nom<br /></span><g:TextBox ui:field="nom"/></label>
  		  <label><span class="noprint">Prénom<br /></span><g:TextBox ui:field="prenom"/></label>
  		  <label><span class="noprint">Date de naissance (aaaa-mm-jj)<br /></span><g:TextBox ui:field="ddn"/></label>
	      <label><span class="noprint">Sexe<br /></span><g:TextBox ui:field="sexe"/></label>
		 </div>
        </fieldset>

		<fieldset> 
		 <legend>Adresse<span id="copySib" class="noprint" style="display:none"> (<a href="#" onclick="copySibling(); return false">copier</a>)</span></legend>
 		 <div class="{style.oneper}">
  		  <label><span class="noprint">Adresse<br /></span><g:TextBox ui:field="adresse" visibleLength="80"/></label>
         </div>
 		 <div class="{style.fiveper}">
  		  <label><span class="noprint">Ville<br /></span><g:TextBox ui:field="ville"/></label>
  		  <label><span class="noprint">Code postal<br /></span><g:TextBox ui:field="codePostal"/></label>
  		  <label>T&eacute;l&eacute;phone(s)<br /><g:TextBox ui:field="tel"/></label>
   		  <label>T&eacute;l&eacute;phone urgence<br /><g:TextBox ui:field="tel_contact_urgence"/></label>
		  <label>Courriel<br /><g:TextBox ui:field="courriel"/></label>
 		 </div>
		</fieldset>

 		<fieldset> <legend>Autres renseignments</legend>
 		 <div class="{style.fourper}">
   		  <label>Num&eacute;ro Judo Qu&eacute;bec<br /><g:TextBox ui:field="affiliation"/></label>
   		  <label>Grade judo <span class="noprint">(<a href="#" onclick="showGradeHistory(); return false">histoire</a>)</span><br /><g:TextBox ui:field="grade"/></label>
	      <label>Date du grade<br /><g:TextBox ui:field="date_grade" visibleLength="10"/></label>
   		  <label>N° arrondissement Anjou<br /><g:TextBox ui:field="carte_anjou"/></label>
   	     </div>
 		 <div class="{style.twoper}">
   		  <label>Nom re&ccedil;u imp&ocirc;t<br /><g:TextBox ui:field="nom_recu_impot" visibleLength="40"/></label>
		 </div>
 		</fieldset>

 		<fieldset> <legend>Inscription</legend>
  		 <div class="{style.fourper}">
   		  <label>Date d'inscription
           <g:Anchor ui:field="today"><span class="noprint">(aujourd'hui)</span></g:Anchor> <span id="desinscrire" class="noprint">(désinscrire)</span><br />
           <g:TextBox ui:field="date_inscription"/>
          </label>
   		  <label>Cat&eacute;gorie d'&acirc;ge<br /><g:TextBox ui:field="categorie"/></label>
   		  <label>Saisons<br /><g:TextBox ui:field="saisons"/></label>
   		  <label>Affiliation Judo QC verifi&eacute;<br /><g:CheckBox ui:field="verification"/>
 		  </label>
  		 </div>
  		
  		 <div class="{style.fourper}">
   		  <label style="width:46%">Cours<br /><g:ListBox ui:field="cours"/></label>
   		  <label>Sessions<br /><g:ListBox ui:field="sessions"/></label>
   		  <label style="float:right">Frais cours<br /><g:TextBox ui:field="categorieFrais"/></label>
  		 </div>
  		 <div class="{style.fourper}">
   		  <label>Escompte<br /><g:ListBox ui:field="escompte"/></label>
          <label>Raison reduction cas sp&eacute;cial<br /><g:TextBox ui:field="cas_special_note"/></label>
   		  <label>Pourcentage escompte cas sp&eacute;cial<br /><g:TextBox ui:field="cas_special_pct"/></label>
          <label style="float:right"> - escompte<br /><g:TextBox ui:field="escompteFrais"/></label>
         </div>
         <div class="{style.fourper}">
   		  <label>Déjà affili&eacute; Judo Qu&eacute;bec 2011-12<br /><g:CheckBox ui:field="sans_affiliation"/></label>
          <label style="float:right">Affiliation Judo Qu&eacute;bec<br /><g:TextBox ui:field="affiliationFrais"/></label>
         </div>
         <div class="{style.fourper}">
          <label>Co&ucirc;t judogi<br /><g:TextBox ui:field="judogi"/></label>
          <label>Passeport Judo Qu&eacute;bec (5 $)<br /><g:CheckBox ui:field="passeport"/></label>
          <label>Pas de carte Anjou (5 $)<br /><g:CheckBox ui:field="non_anjou"/></label>
          <label style="float:right">Autres frais<br /><g:TextBox ui:field="suppFrais"/></label>
        </div>
        <div class="{style.fourper}">
          <label style="float:right">Frais total<br /><g:TextBox ui:field="frais"/></label>
        </div>
       </fieldset>
      </form>
      <g:Button ui:field='saveClientButton'>Sauvegarder</g:Button>
      <g:Button ui:field='discardClientButton'>Annuler</g:Button>

      </g:HTMLPanel>

